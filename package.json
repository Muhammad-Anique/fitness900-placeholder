{
  "name": "fitness900-placeholder",
  "version": "1.0.0",
  "description": "A modern, SEO-optimized placeholder website for Fitness900 - Built with Next.js 14 and TypeScript",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true next build",
    "export": "next export",
    "clean": "rimraf .next out"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Muhammad-Anique/fitness900-placeholder.git"
  },
  "keywords": [
    "fitness900",
    "placeholder",
    "nextjs",
    "typescript",
    "responsive",
    "seo",
    "performance",
    "accessibility",
    "vercel"
  ],
  "author": "Muhammad Anique <contact@muhammadanique.com>",
  "license": "MIT",
  "homepage": "https://fitness900.com",
  "bugs": {
    "url": "https://github.com/Muhammad-Anique/fitness900-placeholder/issues"
  }
